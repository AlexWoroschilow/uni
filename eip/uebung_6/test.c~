
  /* Test des Array_Moduls */
  
#include <stdio.h>
#include <string.h>
#include "./Array_Modul.h"


int main() {
  int array[5];
  int array_length;
  array_length = sizeof(array) / sizeof(int);
  scanArray(array, array_length);
  invertArray(array, array_length);
  sortArray(array, array_length);
  printf("\n");
  return(0);
}